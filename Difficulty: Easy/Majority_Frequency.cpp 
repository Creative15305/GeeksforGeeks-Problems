#include <bits/stdc++.h>
using namespace std;

string majorityFrequencyGroup(string s) {
    unordered_map<char, int> freq;
    for (char c : s) freq[c]++;

    // Group by frequency
    unordered_map<int, vector<char>> groups;
    for (auto& [ch, f] : freq) {
        groups[f].push_back(ch);
    }

    int maxSize = 0, bestFreq = 0;
    for (auto& [f, chars] : groups) {
        int size = chars.size();
        if (size > maxSize || (size == maxSize && f > bestFreq)) {
            maxSize = size;
            bestFreq = f;
        }
    }

    string res = "";
    for (char c : groups[bestFreq]) res += c;
    return res;
}

int main() {
    string s;
    cin >> s;
    cout << majorityFrequencyGroup(s) << endl;
    return 0;
}
