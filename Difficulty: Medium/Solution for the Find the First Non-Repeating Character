#include<iostream>
#include<string>
using namespace std;

int main(){
  string str;
  cout<< "Enter a string:";
  cin>>str;

  int freq[256]={0}; //frequency array for all ASCII characters

//1st traversal: count frequency of each character
  for(char ch:str){
     freq[(int)ch]++;
  }
//2nd traversal: find first character with frequency 1
   char result= '\0';
   for(char ch: str){
      if(freq[(int)ch]==1){
        result= ch;
        break;
      }
   }
  if(result != '\0')
       cout<< "First non-repeating character:"<< result<<endl;
  else
       cout<< "No non-repeating character found."<<endl;
 return 0;
}
