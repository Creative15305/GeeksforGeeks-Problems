#include <iostream>
using namespace std;

int main()
{
    int w, x, y, z;

    cout << "Enter rows and columns of first matrix (w x): ";
    cin >> w >> x;

    cout << "Enter rows and columns of second matrix (y z): ";
    cin >> y >> z;

    if (x != y)
    {
        cout << "Matrix multiplication not possible! (columns of A != rows of B)" << endl;
        return 0;
    }

    int A[w][x], B[y][z], C[w][z];

    cout << "Enter elements of first matrix (A):" << endl;
    for (int i = 0; i < w; i++)
    {
        for (int j = 0; j < x; j++)
        {
            cin >> A[i][j];
        }
    }

    cout << "Enter elements of second matrix (B):" << endl;
    for (int i = 0; i < y; i++)
    {
        for (int j = 0; j < z; j++)
        {
            cin >> B[i][j];
        }
    }

    for (int i = 0; i < w; i++)
    {
        for (int j = 0; j < z; j++)
        {
            C[i][j] = 0;
        }
    }

    for (int i = 0; i < w; i++)
    {
        for (int j = 0; j < z; j++)
        {
            for (int k = 0; k < x; k++)
            {
                C[i][j] += A[i][k] * B[k][j];
            }
        }
    }

    cout << "Resultant matrix (C = A X B):" << endl;
    for (int i = 0; i < w; i++)
    {
        for (int j = 0; j < z; j++)
        {
            cout << C[i][j] << " ";
        }
        cout << endl;
    }

    return 0;
}
